{
  "dataset": {
    "flickr8k": {
      "image_folder": "dataset/flickr8k/Flickr8k_Dataset/",
      "caption_token_file": "dataset/flickr8k/Flickr8k.token.txt",
      "caption_token_file_indonesia": "dataset/flickr8k/Flickr8k.token.indo.txt",
      "train_file": "dataset/flickr8k/Flickr_8k.trainImages.txt",
      "validation_file": "dataset/flickr8k/Flickr_8k.devImages.txt",
      "test_file": "dataset/flickr8k/Flickr_8k.testImages.txt",
      "start_mark": " <start> ",
      "end_mark": " <end> ",
      "batch_size": 32
    },
    "flickr30k": {
      "image_folder": "dataset/flickr30k/flickr30k-images/",
      "caption_token_file": "dataset/flickr30k/Flickr30k.token.txt",
      "caption_token_file_indonesia": "dataset/flickr30k/Flickr30k.token.indo.txt",
      "train_file": "dataset/flickr8k/Flickr_8k.trainImages.txt",
      "validation_file": "dataset/flickr8k/Flickr_8k.devImages.txt",
      "test_file": "dataset/flickr8k/Flickr_8k.testImages.txt",
      "start_mark": " <start> ",
      "end_mark": " <end> ",
      "batch_size": 32
    }
  },
  "modeler": {
    "inceptionv3gru": {
      "learning_rate": 0.001,
      "embedding_output": 128,
      "image_dense": 512,
      "gru_layer": 512
    }
  },
  "serving": {
    "output": "array"
  },
  "preprocessor": {
  },
  "logger": {
    "name": "image-caption",
    "version": 1,
    "disable_existing_loggers": false,
    "handlers": {
      "debug_stdout_handler": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "stream": "ext://sys.stdout",
        "formatter": "verbose"
      },
      "warning_stdout_handler": {
        "class": "logging.StreamHandler",
        "level": "WARNING",
        "stream": "ext://sys.stdout",
        "formatter": "verbose"
      },
      "info_stdout_handler": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "stream": "ext://sys.stdout",
        "formatter": "verbose"
      },
      "info_file_handler": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "INFO",
        "formatter": "verbose",
        "filename": "logs/image_caption_info.log",
        "maxBytes": 1048576,
        "backupCount": 512,
        "encoding": "utf8"
      }
    },
    "formatters": {
      "verbose": {
        "format": "%(name)s - %(asctime)s - %(message)s"
      }
    },
    "loggers": {
      "image_caption": {
        "level": "DEBUG",
        "handlers": [
          "debug_stdout_handler",
          "info_file_handler"
        ],
        "propagate": false
      },
      "tensorflow": {
        "level": "INFO",
        "handlers": [
          "info_stdout_handler",
          "info_file_handler"
        ]
      },
      "root": {
        "level": "WARNING",
        "handlers": [
          "warning_stdout_handler"
        ]
      }
    }
  }
}
